{
    "name": "laramate/tag",
    "description": "Tags for Eloquent Models in Laravel",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Laramate",
            "email": "coding@laramate.de",
            "homepage": "https://www.laramate.de/",
            "role": "Team"
        },
        {
            "name": "Tobi Kivelip",
